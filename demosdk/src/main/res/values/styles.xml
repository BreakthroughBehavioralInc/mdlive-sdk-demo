<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!--MDLive Styles -->
    <!-- ############ Custom Base Style ############ -->
    <style name="cigna_ActivityStyle" parent="rodeo__ActivityStyle">
        <!--ActionBar - customize back/home button. -->
        <item name="homeAsUpIndicator">@drawable/rodeo__activity_cancel_icon</item>
        <item name="windowActionBarOverlay">false</item>
        <!-- Support library compatibility -->
        <item name="rodeo__app_name">@string/cigna</item>
        <!-- Select a Provider Lets get started screen background color. -->
        <item name="rodeo__menu_background">@color/mycigna_blue_logo</item>
        <!-- 'Reason for Visit' Card 1 backgrounf color.-->
        <item name="fwf__card_view_background_color">@android:color/white</item>
        <!-- ActionBar - ProgressBar color -->
        <item name="fwf__wizard_progress_bar_style">@style/cigna__ProgressBar_Horizontal</item>
        <item name="fwf__progress_bar_indeterminate_tint_color">@color/cigna_orange_light</item>
        <!-- ActionBar Title Text Style -->
        <item name="rodeo__action_bar_title_text_style">@style/cigna__ActionBarTitleTextAppearance
        </item>
        <!-- File chooser background color -->
        <item name="fwf__file_chooser_dialog_background">@color/mycigna_blue_logo</item>
        <!--Find a Provider inner Title Text Color, and for Medication, Allergies, -->
        <!--Pre-Conditions, and Procedures default background Text color.-->
        <item name="rodeo__page_title_style">@style/cigna__PageTitleStyle</item>
        <item name="rodeo__page_smaller_instructions_style">
            @style/cigna__PageSmallerInstructionsStyle
