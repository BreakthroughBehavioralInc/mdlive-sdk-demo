        <item name="fwf__EditTextStyle">@style/cigna__EditTextStyle</item>

        <!-- Find a provider, Availability and Consultation Type -->
        <item name="fwf__fab_call_widget_style">@style/cigna__FabWidgetStyleCall</item>
        <item name="rodeo__button_dialog_cancel_style">@style/cigna__ButtonDialogCancelStyle</item>

        <!-- Provider Profile Buttons -->
        <item name="fwf__provider_profile_primary_button_style">
            @style/cigna__ProviderProfilePrimaryButtonStyle
        </item>
        <item name="fwf__provider_profile_secondary_button_style">
            @style/cigna__ProviderProfileSecondaryButtonStyle
        </item>
        <item name="fwf__hyperlink_text_color">@color/cigna_blue2</item>

        <!-- Provider Visit Buttons -->
        <item name="mdl__provider_visit_buttons_style">@style/cigna__ProviderVisitButtonsStyle
