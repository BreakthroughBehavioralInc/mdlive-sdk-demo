        <!--Style for colors in confirm appointment's icons-->
        <item name="fwf__confirm_appointment_style">@style/client_defined__ConfirmAppointmentStyle</item>

        <!--Color for the quality icon in the widgets-->
        <item name="fwf__warning_quality_icon_fill_color">@color/client_defined_quality_alert_orange</item>

