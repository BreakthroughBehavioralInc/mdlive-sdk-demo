        <item name="fwf__hint_text_color">@color/mdl__gray</item>

        <!-- Providers List Cell Style-->
        <item name="mdl__provider_list_cell_style">@style/client_defined__ProviderListCellStyle</item>
        <!-- END Providers List Cell Style-->

        <!--Styles for the number and icon color in the cancel dialog-->
        <item name="fwf__call_number_style">@style/client_defined__CallNumberStyle</item>

        <!--style for pharmacy search screen-->
        <item name="mdl__pharmacy_search_screen_style">@style/client_defined__PharmacySearchScreenStyle</item>

        <!--style for message screens-->
        <item name="mdl__message_center_style">@style/client_defined__MessageCenterStyle</item>

        <!--style for visit history screen-->
        <item name="mdl__visit_history_style">@style/client_defined__VisitHistoryStyle</item>

        <!--style for family member list screen-->
