        <item name="fwf__upload_photo_button_style">@style/client_defined__UploadPhotoButtonStyle</item>
        <!-- Medical History - Card View -->
        <item name="mdl__medical_history_style">@style/client_defined__MedicalHistoryStyle</item>
        <item name="fwf__question_card_view_style">@style/client_defined__QuestionCardViewStyle</item>

        <!-- Availability and consultation radio card views -->
        <item name="fwf__card_view_style">@style/client_defined__CardViewStyle</item>
        <!-- Dashboard Appointment card views -->
        <item name="fwf__appointment_card_view_style">@style/client_defined__AppointmentCardViewStyle</item>
        <!-- See a Family Physician - Search for a Provider Button card views -->
        <item name="fwf__provider_search_button_card_view_style">@style/client_defined__ProviderSearchButtonCardViewStyle</item>

        <!-- Payment and CC Subhead -->
        <item name="fwf__subhead_bar_payment_style">@style/client_defined__SubheadBarPaymentStyle</item>

        <!-- Add Primary Care Physician Button -->
        <item name="mdl__add_primary_care_physician_button_style">@style/client_defined__AddPrimaryCarePhysicianButtonStyle</item>

        <item name="mdl__sso_dashboard_style">@style/client_defined__SsoDashboardStyle</item>

