        <item name="fwf__chip_item_provider_search_criteria_style">@style/client_defined_ChipItemProviderSearchCriteriaStyle</item>

        <!-- TextView text entry cursor color -->
        <item name="fwf__EditTextStyle">@style/client_defined__EditTextStyle</item>

        <!-- Find a provider, Availability and Consultation Type -->
        <item name="fwf__fab_call_widget_style">@style/client_defined__FabWidgetStyleCall</item>
        <item name="rodeo__button_dialog_cancel_style">@style/client_defined__ButtonDialogCancelStyle</item>

        <!-- Provider Profile Buttons -->
        <item name="fwf__provider_profile_primary_button_style">@style/client_defined__ProviderProfilePrimaryButtonStyle</item>
        <item name="fwf__provider_profile_secondary_button_style">@style/client_defined__ProviderProfileSecondaryButtonStyle</item>
        <item name="fwf__hyperlink_text_color">@color/client_defined_blue2</item>

        <!-- Provider Visit Buttons -->
        <item name="mdl__provider_visit_buttons_style">@style/client_defined__ProviderVisitButtonsStyle</item>
        <!-- END Provider Visit Buttons -->
        <!-- Text entry field under-line -->
        <item name="fwf__focus_view_color">@color/client_defined_blue_logo</item>

        <item name="fwf_alert_dialog_style">@style/client_defined__AlertDialogStyle</item>

        <!-- Default provider profile picture -->
