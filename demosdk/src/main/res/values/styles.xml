        <!-- END Default provider profile picture -->
        <item name="mdl__pharmacy_widget_style">@style/client_defined__PharmacyWidgetStyle</item>
        <item name="mdl__find_provider_card_view_style">@style/client_defined__FindProviderCardViewStyle</item>
        <item name="mdl__pharmacy_card_view_style">@style/client_defined__PharmacyCardViewStyle</item>
