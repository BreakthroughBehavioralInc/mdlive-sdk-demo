        <item name="mdl__provider_list_style">@style/sdk_client__ProviderListStyle</item>

        <!--style for the provider profile screen-->
        <item name="mdl__provider_profile_style">@style/sdk_client__ProviderProfileStyle</item>

        <!--Style for all the checkboxes in the app-->
        <item name="fwf__checkbox_style">@style/sdk_client__CheckboxStyle</item>

        <!--Style for colors in confirm appointment's icons-->
        <item name="fwf__confirm_appointment_style">@style/sdk_client__ConfirmAppointmentStyle</item>

        <!--Color for the quality icon in the widgets-->
        <item name="fwf__warning_quality_icon_fill_color">@color/sdk_alert</item>

