        <item name="fwf__fab_widget_background_tint">@android:color/white</item>
        <item name="fwf__fab_widget_disabled_background_tint">@color/black_transparent</item>
        <item name="fwf__fab_widget_icon_tint">@color/sav_sdk_secondary</item>
        <item name="fwf__fab_widget_invalid_icon_tint">@android:color/white</item>
        <!-- Color for the fail background FAB -->
        <item name="fwf__fab_widget_alternative_tint">@color/sdk_alert</item>
        <item name="fwf__fab_widget_ripple_color">@color/mdl__light_gray</item>
        <!-- Ripple effect for touchable views -->
        <item name="colorControlHighlight">@color/mdl__light_gray</item>
        <item name="rodeo__find_provider_check_availability_main_message">
            @string/find_provider_check_availability_main_message
        </item>
        <item name="rodeo__find_provider_check_availability_wait_message">
