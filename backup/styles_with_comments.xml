        <item name="mdl__expired_token_error_title">@string/mdl__expired_token_error_title</item>
        <item name="mdl__expired_token_error_message">@string/mdl__expired_token_error_message
        </item>

        <!-- Search provider chip filter -->
        <item name="fwf__chip_item_provider_list_style">@style/sdk_client_ChipItemProviderListStyle
        </item>
        <item name="fwf__chip_item_provider_search_criteria_style">
            @style/sdk_client_ChipItemProviderSearchCriteriaStyle
        </item>

        <!-- TextView text entry cursor color -->
        <item name="fwf__EditTextStyle">@style/sdk_client__EditTextStyle</item>

        <!-- Find a provider, Availability and Consultation Type -->
        <item name="fwf__fab_call_widget_style">@style/sdk_client__FabWidgetStyleCall</item>
        <item name="rodeo__button_dialog_cancel_style">@style/sdk_client__ButtonDialogCancelStyle</item>

        <!-- Provider Profile Buttons -->
        <item name="fwf__provider_profile_primary_button_style">
            @style/sdk_client__ProviderProfilePrimaryButtonStyle
        </item>
        <item name="fwf__provider_profile_secondary_button_style">
            @style/sdk_client__ProviderProfileSecondaryButtonStyle
        </item>
        <item name="fwf__hyperlink_text_color">@color/sdk_sdk_primary</item>

        <!-- Provider Visit Buttons -->
        <item name="mdl__provider_visit_buttons_style">@style/sdk_client__ProviderVisitButtonsStyle
        </item>
        <!-- END Provider Visit Buttons -->
        <!-- Text entry field under-line -->
        <item name="fwf__focus_view_color">@color/sav_sdk_secondary</item>

        <item name="fwf__call_number_widget_style">
            @style/sdk_client__CallNumberWidgetStyle
        </item>
        <item name="fwf__phone_icon_style">
            @style/sdk_client__PhoneIconStyle
        </item>

        <item name="fwf_alert_dialog_style">
            @style/sdk_client__AlertDialogStyle
        </item>

        <!-- Default provider profile picture -->
        <item name="mdl__default_provider_profile_picture">
            @drawable/sdk_client__provider_profile_picture
        </item>
        <!-- END Default provider profile picture -->
        <item name="mdl__pharmacy_widget_style">@style/sdk_client__PharmacyWidgetStyle</item>
        <item name="mdl__find_provider_card_view_style">@style/sdk_client__FindProviderCardViewStyle
        </item>
        <item name="mdl__pharmacy_card_view_style">@style/sdk_client__PharmacyCardViewStyle</item>
