        <item name="fwf__hint_text_color">@color/mdl__gray</item>

        <!-- Providers List Cell Style-->
        <item name="mdl__provider_list_cell_style">@style/sdk_client__ProviderListCellStyle</item>
        <!-- END Providers List Cell Style-->

        <!--Styles for the number and icon color in the cancel dialog-->
        <item name="fwf__call_number_style">@style/sdk_client__CallNumberStyle</item>

        <!--style for pharmacy search screen-->
        <item name="mdl__pharmacy_search_screen_style">@style/sdk_client__PharmacySearchScreenStyle</item>

        <!--style for message screens-->
        <item name="mdl__message_center_style">@style/sdk_client__MessageCenterStyle</item>

        <!--style for visit history screen-->
        <item name="mdl__visit_history_style">@style/sdk_client__VisitHistoryStyle</item>

        <!--style for family member list screen-->
        <item name="mdl__family_member_list_style">@style/sdk_client__FamilyMemberListStyle</item>

