<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/demo_app_color</item>
        <item name="colorPrimaryDark">@color/demo_app_color</item>
        <item name="colorAccent">@color/demo_app_color</item>
    </style>

    <!--MDLive Styles -->
    <!-- ############ Custom Base Style ############ -->
    <style name="sdk_client_ActivityStyle" parent="rodeo__ActivityStyle">
        <item name="colorAccent">@color/sav_sdk_secondary</item>
        <!--ActionBar - customize back/home button. -->
        <item name="homeAsUpIndicator">@drawable/rodeo__activity_cancel_icon</item>
        <item name="windowActionBarOverlay">false</item>
        <!-- Support library compatibility -->
        <item name="rodeo__app_name">@string/sdk_client</item>
        <!-- Select a Provider Lets get started screen background color. -->
        <item name="rodeo__menu_background">@color/sav_sdk_secondary</item>
        <!-- 'Reason for Visit' Card 1 backgrounf color.-->
        <item name="fwf__card_view_background_color">@android:color/white</item>
        <!-- ActionBar - ProgressBar color -->
        <item name="fwf__wizard_progress_bar_style">@style/sdk_client__ProgressBar_Horizontal</item>
        <item name="fwf__progress_bar_indeterminate_tint_color">@color/sdk_client_orange_light</item>
        <!-- ActionBar Title Text Style -->
        <item name="rodeo__action_bar_title_text_style">@style/sdk_client__ActionBarTitleTextAppearance
        </item>
        <!-- File chooser background color -->
        <item name="fwf__file_chooser_dialog_background">@color/sav_sdk_secondary</item>
        <!--Find a Provider inner Title Text Color, and for Medication, Allergies, -->
        <!--Pre-Conditions, and Procedures default background Text color.-->
        <item name="rodeo__page_title_style">@style/sdk_client__PageTitleStyle</item>
        <item name="rodeo__page_smaller_instructions_style">
            @style/sdk_client__PageSmallerInstructionsStyle
        </item>
        <item name="rodeo__page_smaller_notes_style">
            @style/sdk_client__PageSmallerNotesStyle
        </item>
        <!-- Reason for Visit - Upload Button Style -->
        <item name="fwf__upload_photo_button_style">@style/sdk_client__UploadPhotoButtonStyle</item>
        <!-- Medical History - Card View -->
        <item name="mdl__medical_history_style">@style/sdk_client__MedicalHistoryStyle</item>
        <item name="fwf__question_card_view_style">@style/sdk_client__QuestionCardViewStyle</item>

        <!-- Availability and consultation radio card views -->
        <item name="fwf__card_view_style">@style/sdk_client__CardViewStyle</item>
        <!-- Dashboard Appointment card views -->
        <item name="fwf__appointment_card_view_style">@style/sdk_client__AppointmentCardViewStyle</item>
        <!-- See a Family Physician - Search for a Provider Button card views -->
        <item name="fwf__provider_search_button_card_view_style">
            @style/sdk_client__ProviderSearchButtonCardViewStyle
        </item>

        <!-- Payment and CC Subhead -->
        <item name="fwf__subhead_bar_payment_style">@style/sdk_client__SubheadBarPaymentStyle</item>

        <!-- Add Primary Care Physician Button -->
        <item name="mdl__add_primary_care_physician_button_style">
            @style/sdk_client__AddPrimaryCarePhysicianButtonStyle
        </item>

        <item name="mdl__sso_dashboard_style">@style/sdk_client__SsoDashboardStyle</item>

        <!--Family Memeber's Image default circle place holder.-->
        <item name="fwf__circle_image_default_src">@drawable/fwf__sdk_client_blue_circle</item>
        <!-- Provider Profile background Drawable or Color.-->
        <item name="provider_profile__background">@color/sav_sdk_secondary</item>

        <item name="fwf__user_profile_widget_style">@style/sdk_client__UserProfileWidgetStyle</item>

        <item name="fwf__dashboard_card_view_button_style">
            @style/sdk_client__DashboardCardViewButtonStyle
        </item>

        <item name="mdl__waiting_room_and_video_session_style">
            @style/sdk_client__WaitingRoomAndVideoSessionStyle
        </item>

